!!!
%html{:lang => "en", "xml:lang" => "en", :xmlns => "http://www.w3.org/1999/xhtml"}
  %head
    %meta{:content => "text/html; charset=utf-8", "http-equiv" => "Content-type"}/
    %title Jimmy's Urban Bar and Grill
    %meta{:content => "", :name => "description"}/
    %link{:href => "images/favicon.ico", :rel => "shortcut icon"}/
    %link{:href => "stylesheets/nivo-slider.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "javascripts/fancybox/jquery.fancybox-1.3.4.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "stylesheets/limon.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "stylesheets/colors/orange.css", :rel => "stylesheet", :type => "text/css"}/
    %script{:src => "javascripts/jquery-1.6.2.js", :type => "text/javascript"}
    %script{:src => "javascripts/jquery-ui-1.8.14.js", :type => "text/javascript"}
    %script{:src => "javascripts/jquery.nivo.slider.js", :type => "text/javascript"}
    %script{:src => "javascripts/fancybox/jquery.fancybox-1.3.4.pack.js", :type => "text/javascript"}
    %script{:src => "javascripts/global.js", :type => "text/javascript"}
    :javascript
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-41884879-1', 'lodojimmys.com');
      ga('send', 'pageview');

  %body
    / Start Page Container
    #container_wrap
      / Start Header
      #header
        / Start Logo
        .logo
          %img{:alt => "", :src => "images/logo.png"}/
          / Start Navigation
          %ul#nav
            %li
              %a{:href => "index.html"} Home
            %li
              %a{:href => "about_us.html"} About Us
            %li
              %a{:href => "menu_3.html"} Menu
              %ul
                %li
                  %a{:href => "menu_4.html"} Brunch Menu
                %li
                  %a{:href => "menu_3.html"} Regular Menu
            %li
              %a{:href => "Location.html"} Location
            %li.active
              %a{:href => "contact.html"} Contact
          / End Navigation
        / End Logo
      / End Header
      / Start Page Content
      #contact_us.main_content
        .page_heading
          %h1 Contact Us
        .small_left_column
          %p We are happy to hear from you. Please fill in the information below and we will get back to you shortly.
          .notice
          %form#contact_form{:action => "javascript:;", :method => "post"}
            .row
              %p
                %label Your Name
                %input.text_field{:name => "name", :type => "text"}/
              %p
                %label Your Email
                %input.text_field{:name => "email", :type => "text"}/
            .row
              %p
                %label Your Phone Number
                %input.text_field{:name => "phone", :type => "text"}/
              %p
                %label Subject
                %select.text_field{:name => "subject"}
                  %option Select a subject...
                  %option General
                  %option Complaint
                  %option Catering
                  %option Employment
            %p
              %label Message / Question
              %textarea.text_field{:cols => "10", :name => "message", :rows => "10"}
            %input.button{:type => "submit", :value => "Send Message"}/
        / end left column
        #sidebar.large_right_column
          #hq.container
            %h3 Jimmy's Urban Bar and Grill
            .content
              %ul
                %li.mail
                  %b By E-mail
                  %p
                    Jim@1530blake.com
                %li.phone
                  %b By Phone
                  %p 303-298-5000
        / end right column
      / End Page Content
      / Start Footer
      #footer
        .left
          %p Copyright &copy; Jimmy's Urban Bar and Grill 2013. All Rights Reserved.
        .right
          %ul
            %li.facebook
              %a{:href => "https://www.facebook.com/JimmysUrbanBarandGrill?fref=ts", :target => "_blank"} Facebook
      / End Footer
    / End Page Container